extends clinic_layout

block content
    div(data-role="header")
        h1 Incoming Patients
        a(id="clichome-button", data-role="button", href="/", data-icon="home", data-corners="true", data-iconpos="notext") Home

    ul(data-role="listview", data-filter="true", data-inset="true", id="patientlist", data-input="#filterBasic-input")
        li: a(href="/joannedoe")
            img(src="img/ph.png", alt="Image")
            h2 Joanne Doe 
            p 10 minutes
        li: a(href="/alexmulligan")
            img(src="img/ph.png", alt="Image")
            h2 Alex Mulligan
            p 10 minutes
        li: a(href="/johnharvard")
            img(src="img/ph.png", alt="Image")
            h2 John Harvard
            p 10 minutes
            
block footer
    div(data-role="footer")
        div(data-role="navbar")
            ul
                li: a(href="/incomingpatients") Incoming Patients
                li: a(href="/pastpatients") Past Patients
                li: a(href="/clinicreview") Reviews
                
block after-scripts
    script(src="js/incomingpatients.js")