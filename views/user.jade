html
head
  meta(charset="UTF-8")
  meta(name="viewport", content="width=device-width, initial-scale=1")
  title Find A Doc
  link(rel="stylesheet", href="https://code.jquery.com/mobile/1.4.5/jquery.mobile-1.4.5.min.css")
  link(rel="stylesheet", href="stylesheets/main.css")
  script(src="https://code.jquery.com/jquery-2.1.3.min.js")
  script(src="https://code.jquery.com/mobile/1.4.5/jquery.mobile-1.4.5.min.js")
  
body
  div(data-role="page")
    div(data-role="content")
      div(data-role="header")
        h1 My Profile
        a(data-role="button", href="/home", data-icon="home", data-corners="false") Home
        
    div(id="user-img")
      img(src="img/ph.png",alt="User Image")
      
    div(data-role="collapsible", data-corners="false", class="ui-corner-none")
      h2(class="ui-collapsible-heading ui-collapsible-heading-collapsed")
        a(class="ui-collapsible-heading-toggle ui-btn ui-btn-icon-left ui-btn-up-d" href="#" data-corners="false" data-shadow="false" data-iconshadow="true" data-icon="plus" data-iconpos="left" data-theme="d")
          span(class="ui-btn-text") Patient Profile
      
      div(class="ui-body ui-body-d ui-textalign-left")
        div(class="tablerow")
          div(class="left-table") Name:
          div(class="right-table", id="name")
        div(class="tablerow")
          div(class="left-table") Age:
          div(class="right-table", id="age")
        div(class="tablerow")
          div(class="left-table") Gender:
          div(class="right-table", id="gender")
        div(class="tablerow")
          div(class="left-table") Location:
          div(class="right-table", id="location")
        
    div(data-role="collapsible", data-corners="false", class="ui-corner-none")
      h2(class="ui-collapsible-heading ui-collapsible-heading-collapsed")
        a(class="ui-collapsible-heading-toggle ui-btn ui-btn-icon-left ui-btn-up-d", href="#", data-corners="false", data-shadow="false", data-iconshadow="true", data-icon="plus", data-iconpos="left", data-theme="d")
          span(class="ui-btn-next") Insurance Information
      div(class="ui-body ui-body-d ui-textalign-left")
        div(class="tablerow")
          div(class="left-table") Insurance:
          div(class="right-table", id="insurance")
    
    script(src="js/user.js")
    
    div(data-role="footer")
      p(class="copyright") &copy Copyright 2016
      
